{

	"explorer.width": 80,
	"explorer.icon.enableNerdfont": true,
    "explorer.file.hiddenRules": {
        "patternMatches": [".git/*"]
    },
	"explorer.keyMappings.global": {
        "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
        "s": "open:vsplit",
        "mm": "rename",
        "mc": "copyFile",
        "C": "copyFile",
        "md": "delete",
        "D": "delete",
        "ma": "addFile",
        "mA": "addDirectory"
}

}
